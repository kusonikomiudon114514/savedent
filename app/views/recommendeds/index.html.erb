<p id="notice"><%= notice %></p>

<h1><推薦形態></h1>

<br>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>推薦形態</th>
    </tr>
  </thead>

  <tbody>
    <% @recommendeds.each do |recommended| %>
      <tr>
        <td><%= recommended.name %></td>
        <td><center><%= link_to '詳細', recommended %></center></td>
        <td><center><%= link_to '編集', edit_recommended_path(recommended) %></center></td>
        <td><center><%= link_to '削除', recommended, method: :delete, data: { confirm: 'Are you sure?' } %></center></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>

<table>
<td><pre style="font-size: 13px;"><%= link_to '新規作成', new_recommended_path %></td>
</table>
