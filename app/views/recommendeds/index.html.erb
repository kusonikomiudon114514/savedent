<p id="notice"><%= notice %></p>

<h1>Recommendeds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recommendeds.each do |recommended| %>
      <tr>
        <td><%= recommended.name %></td>
        <td><%= link_to 'Show', recommended %></td>
        <td><%= link_to 'Edit', edit_recommended_path(recommended) %></td>
        <td><%= link_to 'Destroy', recommended, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recommended', new_recommended_path %>
